<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">新規プロトタイプ投稿</h2>
        <%= render 'form' %>
    </div>
  </div>
  <% if @prototype.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@prototype.errors.count, "error") %> prevented this prototype from being saved:</h2>
    <ul>
    <% @prototype.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
    </ul>
  </div>
  <% end %>
</div>